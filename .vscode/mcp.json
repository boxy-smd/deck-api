{
  "servers": {
    "API specification": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "apidog-mcp-server@latest",
        "--oas=localhost:3333/v3/api-docs"
      ]
    },
    "Prisma-Remote": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.prisma.io/mcp"
      ]
    }
  }
}
