{
  "name": "deck-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "tsx watch --env-file .env src/server.ts",
    "prestart": "pnpm run build",
    "start": "node --env-file=.env build/server.js",
    "build": "tsup src --out-dir build --format esm",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run -c vitest.unit.config.ts",
    "test:unit:watch": "vitest -c vitest.unit.config.ts",
    "test:e2e": "vitest run -c vitest.e2e.config.ts",
    "test:e2e:watch": "vitest -c vitest.e2e.config.ts",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx --env-file .env prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.5.4",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.0.5",
    "prisma": "^5.19.1",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
