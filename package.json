    "@scalar/fastify-api-reference": "^1.25.9",
    "@scalar/themes": "^0.9.28",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0",
    "fastify": "^4.28.1",
    "fastify-type-provider-zod": "^2.0.0",
    "tsup": "^8.2.4",
    "pino-pretty": "^11.2.2",
    "supertest": "^7.0.0",
    "zod": "^3.23.8"
  }
